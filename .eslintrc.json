{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-alert": "off",
        "react/jsx-filename-extension" : "off",
        "react/jsx-props-no-spreading" : "off",
        "react/no-access-state-in-setstate": "off"
    }
}
